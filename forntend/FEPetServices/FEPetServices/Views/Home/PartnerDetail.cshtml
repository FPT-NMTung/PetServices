<body class="sticky-header overflow-md-visible">
    <!-- End Header -->

    <main class="main-wrapper">
        <div class="post-single-wrapper position-relative">
            <div class="container">
                <div class="row">
                    <div class="col-lg-7 axil-post-wrapper">
                        <div class="post-heading">
                            <h2 class="title">Giới thiệu chung về @Model?.Partner.LastName </h2>
                            <div class="post-author-avatar" style="width: 350px; border-radius:50%;">
                                    <img src="@Model?.Partner.ImagePartner" style="width: 250px;height:250px;" alt="Author Images">
                             </div>
                            <div class="axil-post-meta" style=" height: 350px;">
                                <div class="post-meta-content" >
                                    <h6 class="author-title" style="margin-bottom:20px;">
                                        Họ và tên :@Model?.Partner.FirstName @Model?.Partner.LastName (@Model?.Partner.PartnerInfoId)
                                    </h6>
                                    <h6 class="author-title" style="margin-bottom:20px;">
                                       Địa chỉ : @Model?.Partner.Commune,@Model?.Partner.District,@Model?.Partner.Province
                                    </h6>
                                    <h6 class="author-title" style="margin-bottom:20px;">
                                       Số điện thoại: @Model?.Partner.Phone
                                    </h6>
                                    <h6 class="author-title"  style="margin-bottom:30px;">
                                      Mô tả chung về bản thân: @Model?.Partner.Descriptions
                                      
                                    </h6>
                                    

                                </div>
                            </div>
                        </div>
                        <div>
                            <h2>Các bằng chứng chỉ :</h2>
                        </div>
                        <div>
                            <img style="width:400px;height:200px;" src="@Model?.Partner.ImageCertificate" >
                        </div>
                        <div>
                            <h3>Đánh giá của khách hàng</h3>
                            <div class="axil-comment-area pro-desc-commnet-area">
                                <h5 class="title">03 khách hàng đánh giá</h5>
                                <ul class="comment-list">
                                    <!-- Start Single Comment  -->
                                    <li class="comment">
                                        <div class="comment-body">
                                            <div class="single-comment">
                                                <div class="comment-img">
                                                    <img src="/assets/images/blog/author-image-4.png" alt="Author Images">
                                                </div>
                                                <div class="comment-inner">
                                                    <h6 class="commenter">
                                                        <a class="hover-flip-item-wrapper" href="#">
                                                            <span class="hover-flip-item">
                                                            <span data-text="Cameron Williamson">Eleanor Pena</span>
                                                            </span>
                                                        </a>
                                                        <span class="commenter-rating ratiing-four-star">
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star empty-rating"></i></a>
                                                    </span>
                                                    </h6>
                                                    <div class="comment-text">
                                                        <p>“We’ve created a full-stack structure for our working workflow processes, were from the funny the century initial all the made, have spare to negatives. ” </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                    <!-- End Single Comment  -->

                                    <!-- Start Single Comment  -->
                                    <li class="comment">
                                        <div class="comment-body">
                                            <div class="single-comment">
                                                <div class="comment-img">
                                                    <img src="/assets/images/blog/author-image-4.png" alt="Author Images">
                                                </div>
                                                <div class="comment-inner">
                                                    <h6 class="commenter">
                                                        <a class="hover-flip-item-wrapper" href="#">
                                                            <span class="hover-flip-item">
                                                            <span data-text="Rahabi Khan">Courtney Henry</span>
                                                            </span>
                                                        </a>
                                                        <span class="commenter-rating ratiing-four-star">
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                    </span>
                                                    </h6>
                                                    <div class="comment-text">
                                                        <p>“We’ve created a full-stack structure for our working workflow processes, were from the funny the century initial all the made, have spare to negatives. ”</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                    <!-- End Single Comment  -->

                                    <!-- Start Single Comment  -->
                                    <li class="comment">
                                        <div class="comment-body">
                                            <div class="single-comment">
                                                <div class="comment-img">
                                                    <img src="/assets/images/blog/author-image-5.png" alt="Author Images">
                                                </div>
                                                <div class="comment-inner">
                                                    <h6 class="commenter">
                                                        <a class="hover-flip-item-wrapper" href="#">
                                                            <span class="hover-flip-item">
                                                            <span data-text="Rahabi Khan">Devon Lane</span>
                                                            </span>
                                                        </a>
                                                        <span class="commenter-rating ratiing-four-star">
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                        <a href="#"><i class="fas fa-star"></i></a>
                                                    </span>
                                                    </h6>
                                                    <div class="comment-text">
                                                        <p>“We’ve created a full-stack structure for our working workflow processes, were from the funny the century initial all the made, have spare to negatives. ” </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                    <!-- End Single Comment  -->
                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4" style="margin-left: 110px;">
                        <!-- Start Sidebar Area  -->
                        <aside class="axil-sidebar-area">

                            <!-- Start Single Widget  -->
                            <div class="axil-single-widget mt--40">
                                <h6 class="widget-title">các dịch vụ bạn có thể tham khảo</h6>

                                <!-- Start Single Post List  -->
                                 @foreach (var service in Model.CaServices)
                                {
                                <div class="content-blog post-list-view mb--20">
                                    <div class="thumbnail">
                                        <a href="~/Home/ServiceDetail?serviceCategoryId=@service.SerCategoriesId">
                                            <img src="@service.Picture" alt="Blog Images">
                                        </a>
                                    </div>
                                    <div class="content">
                                        <h6 class="title"><a href="~/Home/ServiceDetail?serviceCategoryId=@service.SerCategoriesId">@service.SerCategoriesName</a></h6>
                                    </div>
                                </div>
                                }
                                <!-- End Single Post List  -->
                            </div>
                            <!-- End Single Widget  -->
                            <!-- Start Single Widget  -->
                            <div class="axil-single-widget mt--40">
                                <h6 class="widget-title">Top sản phẩm bán chạy nhất của hàng .</h6>
                                <ul class="product_list_widget recent-viewed-product">
                                    <!-- Start Single product_list  -->
                                    @foreach (var product in Model.ListProductTop3)
                                    { <li>
                                        <div class="thumbnail">
                                            <a href="/ProductList/Detail?proId=@product.ProductId">
                                                <img src="@product.Picture" alt="Blog Images">
                                            </a>
                                        </div>
                                        <div class="content">
                                            <h6 class="title"><a href="/ProductList/Detail?proId=@product.ProductId">@product.ProductName</a></h6>
                                            <div class="product-meta-content">
                                                <span class="woocommerce-Price-amount amount">@product.Price VNĐ</span>
                                                <p class="price sold">Đã bán: @product.QuantitySold</p>
                                            </div>
                                        </div>
                                    </li>
                                    }
                                    <!-- End Single product_list  -->
                                </ul>

                            </div>
                            <!-- End Single Widget  -->
                        </aside>
                        <!-- End Sidebar Area -->
                    </div>
                </div>
            </div>
        </div>
        </div>
        <!-- End Blog Area  -->

        <!-- Start Related Blog Area  -->
        <div class="related-blog-area bg-color-white pb--60 pb_sm--40">
            <div class="container">
                <div class="section-title-wrapper mb--70 mb_sm--40 pr--110">
                    <span class="title-highlighter highlighter-primary mb--10"> <i class="fal fa-bell"></i>Các hướng dẫn khác bạn có thể tham khảo</span>
                </div>
                <div class="related-blog-activation slick-layout-wrapper--15 axil-slick-arrow  arrow-top-slide">
                    @foreach (var partner in Model.partner)
                            {
                    <div class="slick-single-layout">
                        <div class="content-blog">
                            <div class="inner">
                                <div class="axil-gallery-activation axil-slick-arrow arrow-between-side">
                                    <!-- Start Single Thumb  -->
                                    <div class="thumbnail">
                                        <a href="~/Home/PartnerDetail?partnerID=@partner.PartnerInfoId">
                                            <img src="@partner?.ImagePartner" alt="Blog Images" style="height:300px; width:300px;">
                                        </a>
                                    </div>
                                    <!-- End Single Thumb  -->
                                </div>
                                <div class="content" style="margin-top:200px;">
                                    <h5 class="title"><a href="~/Home/PartnerDetail?partnerID=@partner.PartnerInfoId">@partner.FirstName @partner.LastName</a></h5>

                                </div>
                            </div>
                        </div>
                    </div>
                    }
                    
                </div>
            </div>
        </div>
        <!-- End Related Blog Area  -->

    </main>
